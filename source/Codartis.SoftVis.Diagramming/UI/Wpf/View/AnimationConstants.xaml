<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                    mc:Ignorable="d">

    <Duration x:Key="DiagramShapeAnimationDuration">0:0:0.3</Duration>
    <Duration x:Key="FastPanAndZoomAnimationDuration">0:0:0.1</Duration>
    <Duration x:Key="MediumPanAndZoomAnimationDuration">0:0:0.5</Duration>
    <Duration x:Key="SlowPanAndZoomAnimationDuration">0:0:.75</Duration>
    <QuadraticEase x:Key="MediumPanAndZoomAnimationEasingFunction" EasingMode="EaseOut"/>
    <QuadraticEase x:Key="SlowPanAndZoomAnimationEasingFunction" EasingMode="EaseOut"/>

</ResourceDictionary>